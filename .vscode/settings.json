{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "eslint.rules.customizations": [{ "rule": "*", "severity": "warn" }],
  "eslint.workingDirectories": [
    { "pattern": "apps/native/*/" },
    { "pattern": "apps/nextjs/*/" },
    { "pattern": "packages/*/" },
    { "pattern": "plugins/*/" },
    { "pattern": "tools/*/" }
  ],
  "tailwindCSS.experimental.configFile": "./tools/tailwind/index.ts",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.preferences.autoImportFileExcludePatterns": [
    "next/router.d.ts",
    "next/dist/client/router.d.ts"
  ],
  "typescript.tsdk": "node_modules/typescript/lib",
  "files.associations": {
    "*.txt": "plaintext",
    "*.hbs": "handlebars"
  },
  "[handlebars]": {
    "editor.defaultFormatter": null
  },
  "cSpell.words": [
    "appts",
    "Authed",
    "automerge",
    "autoprefixer",
    "blefnk",
    "Blefonix",
    "bleverse",
    "callout",
    "CLIENTVAR",
    "clsx",
    "deepmerge",
    "envinfo",
    "fontawesome",
    "fortawesome",
    "gsap",
    "ianvs",
    "instanceof",
    "Kbwps",
    "Korniienko",
    "kysely",
    "lucide",
    "manypkg",
    "Nazarii",
    "nbsp",
    "NEXTAUTH",
    "nextjs",
    "Nomaders",
    "noopener",
    "noreferrer",
    "Peresfer",
    "planetscale",
    "postcss",
    "prettiercache",
    "relivator",
    "reliverse",
    "shadcn",
    "signin",
    "signout",
    "solito",
    "subrouters",
    "superapp",
    "superjson",
    "tailwindcss",
    "tanstack",
    "tauri",
    "tauris",
    "todos",
    "topo",
    "trpc",
    "tsbuildinfo",
    "turborepo",
    "typecheck",
    "typeof",
    "unauthed",
    "Vercel",
    "Wordmark"
  ]
}
