{
  "name": "reliverse",
  "version": "0.4.0",
  "description": "At its current stage, Reliverse CLI is a powerful website builder right in your terminal. You can start from scratch or with a template, setting everything up automatically or customizing it to your exact preferences. With all the tools pre-configured and ready to go, you can build exactly what you envision.",
  "keywords": ["reliverse", "cli"],
  "license": "MIT",
  "author": "blefnk",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "reliverse": "./dist/index.js"
  },
  "files": ["dist"],
  "scripts": {
    "appts": "redrun knip lint format typecheck test build",
    "build": "fse remove --quiet dist && tsup",
    "ci": "bun lint:eslint && bun typecheck && bun test && bun build",
    "dev": "tsx src/index.ts --dev",
    "dev:eslint": "bun eslint --inspect-config",
    "dev:node": "node dist/index.js",
    "format": "redrun format:biome",
    "format:biome": "biome format --write .",
    "knip": "knip",
    "latest": "bun update --latest",
    "lint": "redrun lint:eslint lint:biome",
    "lint:biome": "biome lint --write .",
    "lint:eslint": "eslint --cache --fix .",
    "pub": "bun publish --access public",
    "test": "vitest run",
    "test:dev": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "@inquirer/prompts": "^6.0.1",
    "@reliverse/core": "^0.1.0",
    "@reliverse/fs": "^0.6.0",
    "citty": "^0.1.6",
    "consola": "^3.2.3",
    "giget": "^1.2.3",
    "node-fetch": "^3.3.2",
    "nypm": "^0.3.11",
    "ofetch": "^1.4.0",
    "pathe": "^1.1.2",
    "picocolors": "^1.1.0",
    "random-words": "^2.0.1",
    "simple-git": "^3.27.0",
    "tasuku": "^2.0.1"
  },
  "devDependencies": {
    "@atao60/fse-cli": "^0.1.9",
    "@biomejs/biome": "^1.9.2",
    "@eslint-react/eslint-plugin": "^1.14.2",
    "@eslint/js": "^9.11.0",
    "@next/eslint-plugin-next": "^14.2.13",
    "@stylistic/eslint-plugin": "^2.8.0",
    "@tanstack/eslint-plugin-query": "^5.57.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/eslint__js": "^8.42.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.5.5",
    "eslint": "^9.11.0",
    "eslint-plugin-barrel-files": "^2.1.0",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import-x": "^4.3.0",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-n": "^17.10.3",
    "eslint-plugin-no-comments": "^1.1.10",
    "eslint-plugin-no-relative-import-paths": "^1.5.5",
    "eslint-plugin-perfectionist": "^3.7.0",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "eslint-plugin-readable-tailwind": "^1.8.0",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-sonarjs": "^2.0.2",
    "eslint-plugin-sort": "^3.0.2",
    "eslint-plugin-sort-exports": "^0.9.1",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-yml": "^1.14.0",
    "fs-extra": "^11.2.0",
    "globals": "^15.9.0",
    "knip": "^5.30.5",
    "magicast": "^0.3.5",
    "redrun": "^11.0.5",
    "terser": "^5.33.0",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "typescript": "5.6.2",
    "typescript-eslint": "^8.6.0",
    "vitest": "^2.1.1"
  }
}
