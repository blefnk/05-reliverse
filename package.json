{
  "name": "@reliverse/cli",
  "version": "1.3.22",
  "author": "blefnk",
  "type": "module",
  "description": "A CLI tool that offers a convenient way to bootstrap a new web project and prepare it for work.",
  "scripts": {
    "dev": "bun src/main.ts --dev",
    "db:studio": "drizzle-kit studio",
    "check": "bun typecheck && bun lint && bun format",
    "build:npm": "unbuild && bun build.optim.ts",
    "build:jsr": "bun build.optim.ts --jsr",
    "build": "bun build:npm && bun build:jsr",
    "pub:npm": "bun build.publish.ts",
    "pub:jsr": "bun build.publish.ts --jsr",
    "pub:dry": "bun build.publish.ts --dry-run",
    "pub": "bun publish.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --fix .",
    "lint:i": "eslint --inspect-config",
    "format": "biome check --write .",
    "optimize": "putout dist-npm --fix",
    "attw": "bunx @arethetypeswrong/cli",
    "unpub": "npm unpublish",
    "test": "vitest",
    "knip": "knip"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reliverse/cli.git"
  },
  "module": "./dist-npm/main.js",
  "exports": {
    ".": "./dist-npm/main.js"
  },
  "bin": {
    "reliverse": "dist-npm/main.js"
  },
  "bugs": {
    "url": "https://github.com/reliverse/cli/issues",
    "email": "blefnk@gmail.com"
  },
  "files": ["package.json", "README.md", "LICENSE.md", "dist-npm"],
  "homepage": "https://github.com/reliverse/cli",
  "keywords": ["cli", "reliverse"],
  "license": "MIT",
  "dependencies": {
    "@libsql/client": "^0.14.0",
    "@reliverse/core": "^0.1.0",
    "@reliverse/fs": "^0.6.0",
    "@reliverse/prompts": "^1.3.16",
    "@sinclair/typebox": "^0.34.11",
    "@vercel/sdk": "^1.1.0",
    "async-listen": "^3.0.1",
    "better-sqlite3": "^11.7.0",
    "c12": "^2.0.1",
    "cli-spinners": "^3.2.0",
    "confbox": "^0.1.8",
    "cross-spawn": "^7.0.6",
    "destr": "^2.0.3",
    "detect-package-manager": "^3.0.2",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.2",
    "execa": "^9.5.2",
    "fs-extra": "^11.2.0",
    "globby": "^14.0.2",
    "magic-regexp": "^0.8.0",
    "nanoid": "^5.0.9",
    "node-emoji": "^2.2.0",
    "node-fetch": "^3.3.2",
    "node-fetch-native": "^1.6.4",
    "nypm": "^0.4.1",
    "octokit": "^4.0.2",
    "ofetch": "^1.4.1",
    "open": "^10.1.0",
    "ora": "^8.1.1",
    "pathe": "^1.1.2",
    "picocolors": "^1.1.1",
    "pkg-types": "^1.2.1",
    "querystring": "^0.2.1",
    "random-words": "^2.0.1",
    "simple-git": "^3.27.0",
    "std-env": "^3.8.0",
    "tasuku": "^2.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@cspell/dict-npm": "^5.1.17",
    "@eslint/js": "^9.17.0",
    "@swc/core": "^1.10.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/better-sqlite3": "^7.6.12",
    "@types/bun": "^1.1.14",
    "@types/cross-spawn": "^6.0.6",
    "@types/eslint__js": "^8.42.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.2",
    "@types/strip-comments": "^2.0.4",
    "drizzle-kit": "^0.30.1",
    "eslint": "^9.17.0",
    "eslint-plugin-perfectionist": "^4.3.0",
    "globals": "^15.13.0",
    "knip": "^5.40.0",
    "magicast": "^0.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.0",
    "unbuild": "^3.0.1",
    "vitest": "^2.1.8"
  }
}
