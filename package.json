{
  "name": "@reliverse/cli",
  "version": "1.3.0",
  "author": "blefnk",
  "type": "module",
  "description": "A CLI tool that offers a convenient way to bootstrap a new web project and prepare it for work.",
  "scripts": {
    "start": "bun src/main.ts",
    "dev": "bun src/main.ts --dev",
    "dev:help": "bun src/main.ts help --dev",
    "dev:login": "bun src/main.ts login --dev",
    "dev:logout": "bun src/main.ts logout --dev",
    "check": "bun typecheck && bun lint && bun format",
    "release": "bumpp && bun check && bun pub",
    "build:npm": "unbuild && tsx build.optim.ts",
    "build:jsr": "tsx build.optim.ts --jsr",
    "build": "bun build:npm && bun build:jsr",
    "pub:npm": "tsx build.publish.ts",
    "pub:jsr": "tsx build.publish.ts --jsr",
    "pub:dry": "tsx build.publish.ts --dry-run",
    "pub": "bun pub:npm && bun pub:jsr",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --fix .",
    "lint:i": "eslint --inspect-config",
    "format": "biome check --write .",
    "optimize": "putout dist-npm --fix",
    "attw": "bunx @arethetypeswrong/cli",
    "unpub": "npm unpublish",
    "test": "vitest",
    "knip": "knip"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reliverse/cli.git"
  },
  "module": "./dist-npm/main.js",
  "exports": {
    ".": "./dist-npm/main.js"
  },
  "bin": {
    "reliverse": "./dist-npm/main.js"
  },
  "bugs": {
    "url": "https://github.com/reliverse/cli/issues",
    "email": "blefnk@gmail.com"
  },
  "files": ["package.json", "README.md", "LICENSE.md", "dist-npm"],
  "homepage": "https://github.com/reliverse/cli",
  "keywords": ["cli", "reliverse"],
  "license": "MIT",
  "dependencies": {
    "@hono/node-server": "^1.13.7",
    "@reliverse/core": "^0.1.0",
    "@reliverse/fs": "^0.6.0",
    "@reliverse/prompts": "^1.3.7",
    "@reliverse/relinka": "^1.2.4",
    "@sinclair/typebox": "^0.34.11",
    "async-listen": "^3.0.1",
    "better-auth": "^1.0.10",
    "c12": "^2.0.1",
    "cli-spinners": "^3.2.0",
    "cross-spawn": "^7.0.6",
    "detect-package-manager": "^3.0.2",
    "dotenv": "^16.4.7",
    "execa": "^9.5.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "giget": "^1.2.3",
    "glob": "^11.0.0",
    "hono": "^4.6.12",
    "magic-regexp": "^0.8.0",
    "nanoid": "^5.0.9",
    "node-emoji": "^2.2.0",
    "node-fetch": "^3.3.2",
    "nypm": "^0.4.1",
    "ofetch": "^1.4.1",
    "open": "^10.1.0",
    "ora": "^8.1.1",
    "pathe": "^1.1.2",
    "picocolors": "^1.1.1",
    "querystring": "^0.2.1",
    "random-words": "^2.0.1",
    "simple-git": "^3.27.0",
    "std-env": "^3.8.0",
    "tasuku": "^2.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@cspell/dict-npm": "^5.1.14",
    "@eslint/js": "^9.16.0",
    "@swc/core": "^1.10.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/bun": "^1.1.14",
    "@types/cross-spawn": "^6.0.6",
    "@types/eslint__js": "^8.42.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.1",
    "@types/strip-comments": "^2.0.4",
    "bumpp": "^9.8.1",
    "eslint": "^9.16.0",
    "eslint-plugin-perfectionist": "^4.2.0",
    "globals": "^15.13.0",
    "knip": "^5.39.2",
    "magicast": "^0.3.5",
    "strip-comments": "^2.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0",
    "unbuild": "^2.0.0",
    "vitest": "^2.1.8"
  }
}
